	id		= "com.Satay.SEQWT"
	version 	= "2.0.0">

	<name>Satya's ATDW Mobile App</name>
	<description>My First Mobile app for SEQWT Walking Trails</description>
	<author href="https://github.com/stakasi" email="stakasi@zlab.com.au">Satya Takasi</author>
	<gap:platforms>
